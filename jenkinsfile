pipeline {
        agent any
        stages {
                stage ('build') {
                        steps {
                           git branch: 'branch1', url: 'https://github.com/kirankk6932/central_repo.git'
                                sh '''
                                                echo "this is build stage"
                                                sleep 2
                                   '''
                        }
                }

         stage ('deploy') {
                parallel{

                stage ('deploy1') {
                        steps {
                                sh '''
                                                echo "this is deploy stage"
                                                sleep 2
                                   '''
                        }
                }

               stage ('deploy2') {
                        steps {
                                sh '''
                                                echo "this is deploy stage"
                                                sleep 2
                                   '''
                        }
                }
             }

}

                stage ('test') {
                        steps {
                                sh '''
                                                echo "this is test stage"
                                                sleep 2
                                   '''
                        }
                }
        }
}
